<resources>
    <declare-styleable name="CustomColumnView">
        <attr name="columnLeftText" format="string" />
        <attr name="columnRightText" format="string" />
        <attr name="columnLeftTextSize" format="integer"/>
        <attr name="columnRightTextSize" format="integer"/>
        <attr name="columnLeftTextColor" format="color"/>
        <attr name="columnRightTextColor" format="color"/>
        <attr name="columnLeftIcon" format="reference" />
        <attr name="columnRightIcon" format="reference" />
        <attr name="columnLeftIconShow" format="boolean"/>
        <attr name="columnRightIconShow" format="boolean"/>

    </declare-styleable>
</resources>
