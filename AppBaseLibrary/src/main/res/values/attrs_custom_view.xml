<resources>
    <declare-styleable name="CustomColumnView">
        <attr name="columnLeftText" format="string" />
        <attr name="columnRightText" format="string" />
        <attr name="columnLeftTextSize" format="dimension" />
        <attr name="columnRightTextSize" format="dimension" />
        <attr name="columnLeftTextColor" format="color" />
        <attr name="columnRightTextColor" format="color" />
        <attr name="columnLeftTextTypeface" format="string" />
        <attr name="columnRightTextTypeface" format="string" />
        <attr name="columnLeftIcon" format="reference" />
        <attr name="columnRightIcon" format="reference" />
        <attr name="columnLeftIconShow" format="boolean" />
        <attr name="columnRightIconShow" format="boolean" />

    </declare-styleable>
    <declare-styleable name="CustomTextView">
        <attr name="columnTextTypeface" format="string" />

    </declare-styleable>

    <declare-styleable name="CustomTitleBar">
        <attr name="columnTitleText" format="string" />
        <attr name="columnTitleTextSize" format="dimension" />
        <attr name="columnTitleTextColor" format="color" />
        <attr name="columnTitleTextTypeface" format="string" />

        <attr name="columnTitleRightText" format="string" />
        <attr name="columnTitleRightTextColor" format="color" />
        <attr name="columnTitleRightTextSize" format="dimension" />
        <attr name="columnTitleRightTextTypeface" format="string" />
        <attr name="columnTitleRightTextShow" format="boolean" />

        <attr name="columnTitleLeftIcon" format="reference" />
        <attr name="columnTitleLeftIconShow" format="boolean" />

        <attr name="columnTitleRightIcon" format="reference" />
        <attr name="columnTitleRightIconShow" format="boolean" />

        <attr name="columnTitleRightExtendIcon" format="reference" />
        <attr name="columnTitleRightExtendIconShow" format="boolean" />

    </declare-styleable>
</resources>
